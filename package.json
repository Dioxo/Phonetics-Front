{
	"name": "phonetics",
	"version": "1.0.0",
	"keywords": [
		"composant",
		"component",
		"react-template"
	],
	"repository": {
		"type": "git",
		"directory": "dist"
	},
	"scripts": {
		"standalone": "ts-node ./packaging/index.ts standalone",
		"lint": "eslint ./src/** --fix & exit 0 ",
		"import-cs": "ts-node types/import.ts",
		"pack": "rimraf build publish && npm run build && cpx build/** publish/build && cpx ./*.json publish && cpx .npmrc publish",
		"publishNpmPackage": "npm run pack && cd ./publish && npm publish",
		"refreshVSToken": "vsts-npm-auth -config .npmrc -F -T .npmrc",
		"build": "tsc",
		"start": "ts-node ./packaging/index.ts start"
	},
	"devDependencies": {
		"@types/fs-extra": "^9.0.13",
		"@types/node": "^17.0.25",
		"@types/react": "^17.0.44",
		"@types/react-dom": "^17.0.15",
		"@types/react-redux": "^7.1.23",
		"@types/yargs": "^17.0.10",
		"typescript": "^4.6.3"
	},
	"dependencies": {
		"@emotion/react": "^11.9.0",
		"@emotion/styled": "^11.8.1",
		"@mui/icons-material": "^5.6.0",
		"@mui/material": "^5.6.0",
		"@mui/styles": "^5.6.0",
		"@reduxjs/toolkit": "^1.8.1",
		"@swc/core": "^1.2.170",
		"@types/inversify": "^2.0.33",
		"axios": "^0.26.1",
		"cpx": "^1.5.0",
		"css-loader": "^6.7.1",
		"eslint-webpack-plugin": "^3.1.1",
		"ffmpeg": "^0.0.4",
		"fork-ts-checker-webpack-plugin": "^7.2.6",
		"fs-extra": "^10.1.0",
		"html-webpack-plugin": "^5.5.0",
		"inversify": "^6.0.1",
		"inversify-react": "^1.0.1",
		"mini-css-extract-plugin": "^2.6.0",
		"prettier": "^2.6.2",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-redux": "7.2.8",
		"react-router": "^6.3.0",
		"react-router-dom": "^6.3.0",
		"react-router-native": "^5.3.2",
		"react-toastify": "^8.2.0",
		"redux": "4.1.2",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"sass": "^1.50.1",
		"sass-loader": "^12.6.0",
		"source-map-loader": "^3.0.1",
		"style-loader": "^3.3.1",
		"swc-loader": "^0.2.0",
		"ts-node": "^10.7.0",
		"typescript": "^4.6.3",
		"webpack": "^5.72.0",
		"webpack-dev-server": "^4.8.1",
		"webpack-merge": "^5.8.0",
		"yargs": "^17.4.1"
	},
	"options": {
		"folders": {
			"build": "./build"
		},
		"dev": {
			"src": "./src/index.dev.tsx"
		},
		"publish": {
			"entries": {
				"react": "./src/index.dev.tsx"
			}
		},
		"standalone": {
			"src": "./src/index.dev.tsx",
			"index": "./public/index.html"
		}
	}
}
